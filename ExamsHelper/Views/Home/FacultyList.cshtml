@model IEnumerable<Faculties>
@{
    ViewData["Title"] = "Home Page";
}

<h2>Index</h2>


<table class="table" title="@((string)ViewBag.ChoisenUniver)">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NameOfFaculties)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <a asp-action="GetFaculties" asp-route-id="@item.Id">
                        @Html.DisplayFor(modelItem => item.NameOfFaculties)
                    </a>
                </td>
            </tr>
        }
    </tbody>
</table>
